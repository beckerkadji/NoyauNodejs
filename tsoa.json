{
    "entryFile": "./src/server.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": ["./app/controllers/*controller.ts"],
    "spec": {
        "outputDirectory": "build",
        "specVersion": 3
    },
    "routes": {
        "routesDir": "./router",
        "basePath": "/",
        "entryFile": "./src/server.ts"
    },
    "swagger": {
        "outputDirectory": ".",
        "entryFile": "./src/server.ts"
    }
}